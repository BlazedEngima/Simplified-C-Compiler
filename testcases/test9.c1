int numbers[5];
int size = 5;
int sum = 0;
int average;
int n1=1, n2=2, n3=3, n4=4, n5=5;
int i;
numbers[0] = n1;
numbers[1] = n2;
numbers[2] = n3;
numbers[3] = n4;
numbers[4] = n5;

i = 0;
while (i < size) {
    sum = sum + numbers[i];
    i = i + 1;
}

average = sum / size;

printf(average);
return;
